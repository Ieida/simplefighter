[sub_resource type="GDScript" id="GDScript_etmds"]
resource_name = "player"
script/source = "extends CharacterBody2D


signal died


@export var speed = 50.0
@onready var sprite: Sprite2D = $Sprite2D
@onready var animation_player: AnimationPlayer = $AnimationPlayer
var can_move: bool = true
var can_attack: bool = true
var is_attacking: bool
var attack_count: int
@export var hitbox: Hitbox
var id: int
var input: float


func _is_input_peer() -> bool:
	return multiplayer.has_multiplayer_peer() and multiplayer.get_unique_id() == id


func _ready():
	hitbox.took_damage.connect(_on_hitbox_took_damage)
	hitbox.health_depleted.connect(_on_hitbox_health_depleted)
	animation_player.animation_finished.connect(_on_animation_finished)
	if not is_multiplayer_authority():
		request_state_sync.rpc_id(1)


@rpc(\"any_peer\", \"call_local\", \"reliable\")
func request_state_sync():
	var i = multiplayer.get_remote_sender_id()
	sync_state.rpc_id(i, id, hitbox.health)


@rpc(\"any_peer\", \"call_local\", \"unreliable_ordered\")
func _send_input(inp: float):
	input = inp


@rpc(\"authority\", \"call_local\", \"reliable\")
func set_id(new_id: int):
	id = new_id
	if id == -1:
		$ReadyLabel.show()
		modulate = modulate.lerp(Color.BLACK, 0.5)
		hitbox.monitorable = false
	else:
		$ReadyLabel.hide()
		modulate = Color.WHITE
		hitbox.monitorable = true


@rpc(\"authority\", \"call_local\", \"reliable\")
func sync_state(i: int, h: int):
	set_id(i)
	hitbox.health = h


@rpc(\"authority\", \"call_local\", \"unreliable_ordered\")
func _sync_position(pos: Vector2):
	global_position = pos


func _physics_process(_delta):
	if can_move:
		var direction = Input.get_axis(&\"move_left\", &\"move_right\")
		if _is_input_peer():
			_send_input.rpc(direction)
		else:
			direction = input
		
		if direction:
			velocity.x = direction * speed
		else:
			velocity.x = move_toward(velocity.x, 0, speed)
	
	move_and_slide()
	
	if can_move:
		if velocity.x:
			animation_player.play(&\"run\")
			sprite.flip_h = velocity.x < 0.0
			sprite.offset.x = -52 if velocity.x < 0.0 else 52
			$Attacks.scale.x = -1 if velocity.x < 0.0 else 1
		else:
			animation_player.play(&\"idle\")
	
	if multiplayer.has_multiplayer_peer() and is_multiplayer_authority():
		_sync_position.rpc(global_position)
	
	if _is_input_peer() and can_attack:
		if Input.is_action_just_pressed(&\"attack\"):
			attack.rpc()


func _on_animation_finished(anim_name: StringName):
	if anim_name == &\"damaged\":
		if hitbox.health > 0:
			can_move = true
			can_attack = true
			set_physics_process(true)
	elif anim_name == &\"die\":
		hide()
		process_mode = PROCESS_MODE_DISABLED
		died.emit()


func _on_hitbox_health_depleted():
	can_move = false
	can_attack = false
	velocity = Vector2.ZERO
	set_physics_process(false)
	animation_player.play(&\"RESET\")
	animation_player.queue(&\"die\")


func _on_hitbox_took_damage():
	can_move = false
	can_attack = false
	velocity = Vector2.ZERO
	set_physics_process(false)
	animation_player.stop()
	animation_player.play(&\"RESET\")
	animation_player.queue(&\"damaged\")


@rpc(\"any_peer\", \"call_local\", \"reliable\")
func attack():
	if not is_multiplayer_authority() or multiplayer.get_remote_sender_id() != id or not can_attack: return
	
	if attack_count == 0:
		attack_1.rpc()
	elif attack_count == 1:
		attack_2.rpc()
	elif attack_count == 2:
		attack_3.rpc()


@rpc(\"authority\", \"call_local\", \"reliable\")
func attack_1():
	velocity = Vector2.ZERO
	can_move = false
	is_attacking = true
	attack_count = 1
	animation_player.play(&\"RESET\")
	animation_player.queue(&\"attack_1\")
	await animation_player.animation_finished
	can_move = true
	is_attacking = false
	attack_count = 0
	animation_player.play(&\"idle\")


@rpc(\"authority\", \"call_local\", \"reliable\")
func attack_2():
	velocity = Vector2.ZERO
	can_move = false
	is_attacking = true
	attack_count = 2
	animation_player.play(&\"RESET\")
	animation_player.queue(&\"attack_2\")
	await animation_player.animation_finished
	can_move = true
	is_attacking = false
	attack_count = 0
	animation_player.play(&\"idle\")


@rpc(\"authority\", \"call_local\", \"reliable\")
func attack_3():
	velocity = Vector2.ZERO
	can_move = false
	is_attacking = true
	attack_count = 3
	animation_player.play(&\"RESET\")
	animation_player.queue(&\"attack_3\")
	await animation_player.animation_finished
	can_move = true
	is_attacking = false
	attack_count = 0
	animation_player.play(&\"idle\")


@rpc(\"any_peer\", \"call_local\", \"reliable\")
func respawn(pos: Vector2):
	if multiplayer.get_remote_sender_id() != 1: return
	
	global_position = pos
	hitbox.health = hitbox.max_health
	velocity = Vector2.ZERO
	animation_player.play(&\"RESET\")
	process_mode = PROCESS_MODE_INHERIT
	await animation_player.animation_finished
	can_move = true
	can_attack = true
	set_physics_process(true)
	animation_player.queue(&\"idle\")
	show()
	attack_count = 0


@rpc(\"authority\", \"call_remote\", \"unreliable_ordered\")
func sync(vel: Vector2):
	velocity = vel
"

